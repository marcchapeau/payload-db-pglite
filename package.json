{
  "name": "payload-db-pglite",
  "version": "0.1.2",
  "description": "PGlite database adapter for PayloadCMS",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "payloadcms",
    "pglite",
    "database",
    "adapter",
    "postgres",
    "embedded"
  ],
  "author": "Marc Chapeau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcchapeau/payload-db-pglite.git"
  },
  "bugs": {
    "url": "https://github.com/marcchapeau/payload-db-pglite/issues"
  },
  "homepage": "https://github.com/marcchapeau/payload-db-pglite#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "peerDependencies": {
    "@electric-sql/pglite": ">=0.3.0",
    "@payloadcms/drizzle": ">=3.0.0",
    "drizzle-orm": ">=0.43.0",
    "payload": ">=3.0.0"
  },
  "devDependencies": {
    "@electric-sql/pglite": "0.3.15",
    "@payloadcms/drizzle": "3.76.1",
    "@types/node": "20.17.32",
    "drizzle-orm": "0.44.7",
    "payload": "3.76.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {}
}
